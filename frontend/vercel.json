{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    { "src": "/(.+\\.[A-Za-z0-9]+)", "dest": "frontend/dist/$1" },
    { "src": "/(.*)", "dest": "frontend/dist/index.html" }
  ]
}
