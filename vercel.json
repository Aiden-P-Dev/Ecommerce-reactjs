{
  "version": 2,
  "trailingSlash": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.js"
    },

    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|eot|ttf|woff|woff2))",
      "dest": "/frontend/dist/$1"
    },

    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html",
      "status": 200
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
