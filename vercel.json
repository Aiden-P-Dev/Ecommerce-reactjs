{
  "version": 2,
  "trailingSlash": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/src/index.js"
    },

    {
      "src": "/assets/(.*)",
      "dest": "/frontend/dist/assets/$1"
    },

    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html",
      "status": 200
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
