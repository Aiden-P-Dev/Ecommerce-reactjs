{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/src/index.js"
    },

    {
      "src": "/assets/(.*)",
      "dest": "frontend/dist/assets/$1"
    },

    {
      "src": "/",
      "dest": "frontend/dist/index.html"
    },

    {
      "src": "/(.*)",
      "dest": "frontend/dist/index.html"
    }
  ]
}
